<h1>Listing order_to_warehouses</h1>

<table>
  <tr>
    <th>Order status</th>
    <th>Time stamp</th>
    <th>Warehouse</th>
    <th>Store</th>
    <th>Warehouse product info</th>
    <th>Store product info</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @order_to_warehouses.each do |order_to_warehouse| %>
  <tr>
    <td><%= order_to_warehouse.order_status %></td>
    <td><%= order_to_warehouse.time_stamp %></td>
    <td><%= order_to_warehouse.warehouse %></td>
    <td><%= order_to_warehouse.store %></td>
    <td><%= order_to_warehouse.warehouse_product_info %></td>
    <td><%= order_to_warehouse.store_product_info %></td>
    <td><%= link_to 'Show', order_to_warehouse %></td>
    <td><%= link_to 'Edit', edit_order_to_warehouse_path(order_to_warehouse) %></td>
    <td><%= link_to 'Destroy', order_to_warehouse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

